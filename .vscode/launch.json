    "version": "0.2.0",
    "configurations": [
        {
            "name": "C#: Compute.Samples Debug (Dedicated GPU)",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/Compute.Samples/bin/Debug/net9.0/Compute.Samples.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Compute.Samples",
            "env": {
                "DRI_PRIME": "1",
                "__NV_PRIME_RENDER_OFFLOAD": "1",
                "__GLX_VENDOR_LIBRARY_NAME": "nvidia"
