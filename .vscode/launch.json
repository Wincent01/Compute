            "preLaunchTask": "build"
        },
        {
            "name": "C#: Compute.Samples Debug (Integrated GPU)",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/Compute.Samples/Compute.Samples.csproj"
        }

